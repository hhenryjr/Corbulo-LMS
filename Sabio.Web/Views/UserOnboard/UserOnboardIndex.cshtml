        sabio.page.ethnicitiesDropDown = function (data) {
            for (var index = 0; index < data.items.length; index++) {
                var html = '<option value="' + data.items[index].id + '">' + data.items[index].ethnicity + '</option>';
                $('#ethnicityId').append(html);
            }
            $("#ethnicityId").val(null);
        }

        sabio.page.countriesDropDown = function (data) {
            for (var index = 0; index < data.items.length; index++) {
                var html = '<option value="' + data.items[index].id + '">' + data.items[index].name + '</option>';
                $('#countryId').append(html);
            }
            $("#countryId").val(null);
            sabio.page.getId();
        }

        sabio.page.handlers.onSelectState = function () {
            var id = $("#countryId").find(":selected").val();
            $('#stateProvinceId').empty();
            sabio.services.CountryState.getStateProvince(id, sabio.page.stateProvinceDropDown, sabio.page.ajaxError)
        }

        sabio.page.stateProvinceDropDown = function (data) {
            for (var index = 0; index < data.items.length; index++) {
                var html = '<option value="' + data.items[index].stateProvinceId + '">' + data.items[index].name + '</option>';
                $('#stateProvinceId').append(html);
            }
            $('#stateProvinceId').val(null).trigger('change');

            if (sabio.page.stateProvinceId) {

                $("#stateProvinceId").val(sabio.page.stateProvinceId);
                $("#stateProvinceId").trigger("change");
                sabio.page.stateProvinceId = null;
            }
        }

